{
	"": {
		"controller": "Default",
		"method": "start"
	},

	"posts/:page": {
		"controller": "Post",
		"method": "getAllWithPage",
		"params": {
			"page": "number"
		}
    },
    "posts": {
    	"controller": "Post",
    	"method": "getAll"
    },
    "post/:id": {
    	"controller": "Post",
    	"method": "get",
    	"params": {
    		"id": "number"
		}
	},

    "login": {
    	"controller": "User",
    	"method": "login"
	},
	
	"logout": {
    	"controller": "User",
    	"method": "logout"
	}
}

